{
  "name": "@greenlight-coop/greenlight-utils",
  "version": "0.1.22",
  "description": "Shared Node.js module for common production code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "suppress_audit": "false"
  },
  "scripts": {
    "audit": "$($npm_package_config_suppress_audit) || npm audit --production",
    "build": "npm run tsc",
    "build-clean": "npm run clean && npm run build",
    "check-types": "tsc --noEmit",
    "clean": "npx rimraf dist coverage",
    "lint": "eslint --ext ts --config .eslintrc.json .",
    "lint:fix": "npm run lint -- --fix",
    "qc": "npm run lint && npm run audit && npm run check-types && npm run test:coverage",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenlight-coop/greenlight-utils.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "Green Light Software Cooperative",
  "bugs": {
    "url": "https://github.com/greenlight-coop/greenlight-utils/issues"
  },
  "homepage": "https://github.com/greenlight-coop/greenlight-utils#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.11.26",
    "@types/uuid": "^8.3.4",
    "cassandra-driver": "^4.6.3",
    "cloudevents": "^6.0.1",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@greenlight-coop/greenlight-dev": "^0.1.13"
  },
  "prettier": "@greenlight-coop/prettier-config-greenlight"
}
