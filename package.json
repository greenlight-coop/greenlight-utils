{
  "name": "@greenlight-coop/greenlight-utils",
  "version": "0.1.1",
  "description": "Shared Node.js module for common production code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc",
    "build-clean": "npm run clean && npm run build",
    "check-types": "tsc --noEmit",
    "clean": "npx rimraf dist",
    "lint": "eslint --ext ts --config .eslintrc.json .",
    "lint:fix": "npm run lint -- --fix",
    "qc": "npm run lint && npm audit && npm run check-types && npm test",
    "test": "jest",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenlight-coop/greenlight-utils.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "Green Light Software Cooperative",
  "bugs": {
    "url": "https://github.com/greenlight-coop/greenlight-utils/issues"
  },
  "homepage": "https://github.com/greenlight-coop/greenlight-utils#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.25",
    "express": "^4.17.3",
    "typescript": "^4.5.5",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "tiny-types": "^1.17.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0"
  }
}
