{
  "name": "@greenlight-coop/greenlight-utils",
  "version": "0.1.7",
  "description": "Shared Node.js module for common production code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run tsc",
    "build-clean": "npm run clean && npm run build",
    "check-types": "tsc --noEmit",
    "clean": "npx rimraf dist coverage",
    "lint": "eslint --ext ts --config .eslintrc.json .",
    "lint:fix": "npm run lint -- --fix",
    "qc": "npm run lint && npm audit && npm run check-types && npm test",
    "test": "c8 --check-coverage --lines 90 --functions 90 --branches 80 --src src --exclude **/*.spec.ts --reporter text --reporter html jest",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenlight-coop/greenlight-utils.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "Green Light Software Cooperative",
  "bugs": {
    "url": "https://github.com/greenlight-coop/greenlight-utils/issues"
  },
  "homepage": "https://github.com/greenlight-coop/greenlight-utils#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16.11.25",
    "cloudevents": "^5.3.2",
    "express": "^4.17.3",
    "express-actuator": "^1.8.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "typescript": "^4.5.5",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@greenlight-coop/greenlight-dev": "0.1.9",
    "@types/express-actuator": "^1.8.0"
  },
  "prettier": "@greenlight-coop/prettier-config-greenlight"
}
